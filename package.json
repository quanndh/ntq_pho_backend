{
  "name": "nest-starter-kit",
  "version": "0.0.1",
  "description": "Nest Starter Kit",
  "author": "Harry Duong <thedv91@gmail.com>",
  "repository": {
    "url": "https://gitlab.com/thedv/nest-starter-kit",
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": " nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.js \"src/**\"",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:migration:create": "yarn typeorm migration:create -d src/migrations",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/typeorm_migration.config.ts",
    "migration:run": "yarn typeorm:cli migration:run -t=false"
  },
  "dependencies": {
    "@digikare/nestjs-prom": "0.2.2",
    "@elastic/elasticsearch": "^7.12.0",
    "@grpc/proto-loader": "0.5.4",
    "@mux/mux-node": "^3.2.1",
