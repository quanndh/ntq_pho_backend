    "helmet": "3.23.1",
    "jwt-decode": "2.2.0",
    "mime-types": "2.1.27",
    "multer": "1.4.2",
    "nanoid": "3.1.10",
    "nest-access-control": "2.0.1",
    "nestjs-typeorm-paginate": "2.0.3",
