    "class-validator": "^0.12.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "date-fns": "^2.16.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "exceljs": "^4.1.1",
    "google-auth-library": "^6.0.6",
    "google-libphonenumber": "^3.2.13",
    "googleapis": "^73.0.0",
    "graphql": "15.2.0",
    "graphql-query-complexity": "0.6.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-type-json": "0.3.2",
    "grpc": "1.24.3",
